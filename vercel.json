{
    "rewrites": [
        {
            "source": "/api/:path(.*)",
            "destination": "https://staging.backend.yachtfull.com/api/:path"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        },
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ]
}
